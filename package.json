    "install": "^0.13.0",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lottie-react": "^2.3.1",
    "moment": "^2.29.4",
    "nanoid": "^3.3.4",
    "next": "^13.0.6",
    "next-auth": "^4.22.1",
    "next-redux-wrapper": "^8.0.0",
    "next-seo": "^5.14.1",
    "next-sitemap": "^3.1.32",
    "npm": "^9.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
